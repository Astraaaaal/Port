{
    "headers": [
        {
            "source": "/(.*).pdf",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "assets/pdf"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}